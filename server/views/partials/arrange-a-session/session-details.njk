{{ govukSummaryList({
  classes: 'govuk-!-margin-bottom-9',
  rows: [
    {
      key: { text: "Type of appointment" },
      value: { text: typeOfSession }
    },
    {
      key: { text: "Date" },
      value: { text: date | dateWithDayAndWithoutYear }
    },
    {
      key: { text: "Time" },
      value: { text: startTime }
    },
    {
      key: { text: "RAR activity" },
      value: { text: rarCategory }
    } if rarCategory,
    {
      key: { text: "RAR activity subcategory" },
      value: { text: rarSubCategory }
    } if rarCategory,
    {
      key: { text: "Counts towards RAR?" },
      value: { text: 'Yes' if countsTowardsRAR else 'No' }
    },
    {
      key: { text: "Calendar note" },
      value: { text: sessionNotes or 'None' }
    }
  ]
  }) 
}}
