{
  "name": "hmpps-manage-supervisions",
  "version": "0.0.1",
  "repository": "git@github.com:ministryofjustice/hmpps-manage-supervisions.git",
  "description": "Manage supervisions",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run openapi-generate",
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "clean": "rimraf dist",
    "build": "concurrently \"npm:build:*\"",
    "build:server": "nest build",
    "build:client": "webpack -c ./webpack.config.client.js --mode production",
    "start": "node -r dotenv/config dist/main",
    "start:local": "DOTENV_CONFIG_PATH=local.env npm start",
    "start:e2e": "DOTENV_CONFIG_PATH=e2e.env npm start",
    "start:e2e:dev": "NODE_ENV=development DOTENV_CONFIG_PATH=e2e.env npm run start:dev",
    "dev:server": "nest start --watch --debug -e \"node -r dotenv/config\"",
    "dev:client": "webpack -c ./webpack.config.client.js --watch --mode development",
    "start:dev": "concurrently \"npm:dev:*\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --cache --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cypress run",
    "test:e2e-ui": "cypress open",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "openapi-generate": "openapi-generator-cli generate",
    "seed": "ts-node-transpile-only scripts/dev.seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.2",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.1.2",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.3",
    "@ntegral/nestjs-sentry": "^3.0.6",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@sentry/types": "^6.15.0",
    "applicationinsights": "^2.1.9",
    "axios": "^0.21.4",
    "axios-retry": "^3.2.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.0.0",
    "csurf": "^1.11.0",
    "cypress-axe": "^0.13.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "handy-redis": "^2.3.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^2.1.1",
    "node-cache": "^5.1.2",
    "nunjucks": "^3.2.3",
    "openid-client": "^5.0.2",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.6.1",
    "qs": "^6.10.1",
    "redis": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.1",
    "source-map-support": "^0.5.21",
    "striptags": "^3.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@ministryofjustice/frontend": "^1.0.1",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.3",
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "@sentry/webpack-plugin": "^1.18.3",
    "@types/connect-flash": "^0.0.37",
    "@types/connect-redis": "0.0.17",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.9",
    "@types/flat": "^5.0.2",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/luxon": "^2.0.7",
    "@types/node": "^16.11.11",
    "@types/nunjucks": "^3.2.0",
    "@types/passport-oauth2": "^1.4.11",
    "@types/redis": "^2.8.32",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^10.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "accessible-autocomplete": "^2.0.3",
    "axe-core": "^4.3.5",
    "axios-mock-adapter": "^1.20.0",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "cypress": "^8.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "flat": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "govuk-frontend": "^3.14.0",
    "husky": "^7.0.4",
    "jest": "^27.4.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "memfs": "^3.4.0",
    "mini-css-extract-plugin": "^2.4.5",
    "mocha-junit-reporter": "^2.0.2",
    "nock": "^13.2.1",
    "prettier": "^2.5.0",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0",
    "yargs": "^17.2.1"
  },
  "engines": {
    "node": "^16",
    "npm": ">=7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_results"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "./*.json": "prettier --write",
    "!(wiremock)/**/!(*community-api|assess-risks-and-needs-api|delius-api).json": "prettier --write"
  }
}
