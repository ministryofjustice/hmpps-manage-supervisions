{
  "name": "hmpps-manage-supervisions",
  "version": "0.0.1",
  "repository": "git@github.com:ministryofjustice/hmpps-manage-supervisions.git",
  "description": "Manage Supervisions",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "clean": "rimraf dist",
    "build": "nest build",
    "start": "node -r dotenv/config dist/main",
    "start:dev": "nest start --watch -e \"node -r dotenv/config\"",
    "start:debug": "nest start --watch --debug -e \"node -r dotenv/config\"",
    "start:e2e": "DOTENV_CONFIG_PATH=e2e.env npm start",
    "start:e2e:dev": "DOTENV_CONFIG_PATH=e2e.env npm run start:dev",
    "record-build-info": "node scripts/record-build-info.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --cache --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cypress run --config video=false",
    "test:e2e-ui": "cypress open",
    "security_audit": "npx audit-ci --config audit-ci.json"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^0.2.2",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "applicationinsights": "^2.0.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^5.2.0",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "govuk-frontend": "^3.11.0",
    "handy-redis": "^2.2.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "nunjucks": "^3.2.3",
    "openid-client": "^4.7.3",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "qs": "^6.10.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/connect-redis": "0.0.16",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.22",
    "@types/jquery": "^3.5.5",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.26.5",
    "@types/node": "^15.0.3",
    "@types/nunjucks": "^3.1.4",
    "@types/passport-oauth2": "^1.4.10",
    "@types/redis": "^2.8.28",
    "@types/sinon": "^10.0.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "accessible-autocomplete": "^2.0.3",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "cypress": "^7.3.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^5.5.3",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha-junit-reporter": "^2.0.0",
    "nock": "^13.0.11",
    "prettier": "^2.2.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.1.0",
    "sinon": "^10.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "^14",
    "npm": "^6.4.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{json}": "prettier --write"
  }
}
