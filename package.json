{
  "name": "hmpps-manage-supervisions",
  "version": "0.0.1",
  "repository": "git@github.com:ministryofjustice/hmpps-manage-supervisions.git",
  "description": "Manage Supervisions",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run openapi-generate",
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "clean": "rimraf dist",
    "build": "nest build",
    "start": "node -r dotenv/config dist/main",
    "start:dev": "nest start --watch -e \"node -r dotenv/config\"",
    "start:debug": "nest start --watch --debug -e \"node -r dotenv/config\"",
    "start:e2e": "DOTENV_CONFIG_PATH=e2e.env npm start",
    "start:e2e:dev": "DOTENV_CONFIG_PATH=e2e.env npm run start:dev",
    "record-build-info": "node scripts/record-build-info.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --cache --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cypress run",
    "test:e2e-ui": "cypress open",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "openapi-generate": "openapi-generator-cli generate"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.17",
    "applicationinsights": "^2.1.3",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "handy-redis": "^2.2.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "nunjucks": "^3.2.3",
    "openid-client": "^4.7.3",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.5.0",
    "qs": "^6.10.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "retry-axios": "^2.4.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@ministryofjustice/frontend": "^0.2.4",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.17",
    "@openapitools/openapi-generator-cli": "^2.3.5",
    "@types/connect-redis": "0.0.16",
    "@types/csurf": "^1.11.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.5.5",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.5",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/lodash": "^4.14.170",
    "@types/luxon": "^1.26.5",
    "@types/node": "^14.17.0",
    "@types/nunjucks": "^3.1.4",
    "@types/passport-oauth2": "^1.4.10",
    "@types/redis": "^2.8.30",
    "@types/sinon": "^10.0.0",
    "@types/supertest": "^2.0.11",
    "@types/timepicker": "^1.13.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "accessible-autocomplete": "^2.0.3",
    "axios-mock-adapter": "^1.19.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.5",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cypress": "^7.6.0",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "file-loader": "^6.2.0",
    "govuk-frontend": "^3.13.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-junit": "^12.0.0",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha-junit-reporter": "^2.0.0",
    "nock": "^13.0.11",
    "prettier": "^2.3.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "sinon": "^11.1.1",
    "supertest": "^6.1.3",
    "timepicker": "^1.13.18",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": "^14",
    "npm": "^7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test_results"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{json}": "prettier --write"
  }
}
